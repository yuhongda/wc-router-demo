  "main": "src/main.js",
  "scripts": {
    "rm:dist": "rm -rf ./dist",
    "wr": "npm run dev --WHY_RENDER",
    "start": "npm run dev",
    "dev": "npm run mock | webpack-dev-server --config ./config/webpack.config.dev.babel.js --inline --hot --progress",
    "dev:micro": "npm run mock | webpack-dev-server --config ./config/webpack.config.micro.babel.js --inline --hot --progress",
    "build": "npm run rm:dist && webpack --config ./config/webpack.config.prod.babel.js --progress --hide-modules",
    "build:test": "webpack --config ./config/webpack.config.test.babel.js --progress --hide-modules",
    "build:pre": "webpack --config ./config/webpack.config.pre.babel.js --progress --hide-modules",
    "build:online": "npm run rm:dist && OnlineBuild=true webpack --config ./config/webpack.config.prod.babel.js --progress --hide-modules",
    "mock": "nodemon ./mock/app.js",
    "gl": "NODE_ENV=development node ./graphql/",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "stylelint": "stylelint src/**/*.css src/**/*.less src/**/*.scss",
    "format": "npm run format:js && npm run format:css",
    "format:js": "eslint --fix --ext .jsx,.js,.ts,.tsx src/**/*.{js,jsx,ts,tsx}",
    "format:css": "prettier-stylelint --write src/**/*.{css,less,scss}",
    "test": "jest --config=test/jest.config.js",
    "coverage": "jest --coverage --config=test/jest.config.js",
    "test:e2e": "jest --config=test/jest.config.e2e.js",
    "e2e": "node ./test/run-e2e.js",
    "dev:snowpack": "cross-env NODE_OPTIONS=--max_old_space_size=4096 IS_SNOWPACK=true snowpack dev"
  },
