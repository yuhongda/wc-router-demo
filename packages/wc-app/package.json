{
  "name": "wc-app",
  "version": "1.0.0",
  "private": true,
  "description": "大促趋势词",
  "author": "yuhongda",
  "license": "ISC",
  "scripts": {
    "dev": "vite",
    "build": "npm run cjs && npm run esm && npm run dts && npm run style",
    "build:web": "cross-env NODE_ENV=production vite build",
    "cjs": "cross-env NODE_ENV=production FORMAT=cjs rollup -c build/rollup.config.js -o package/cjs/wc-app.js --inlineDynamicImports && npm run cjs:index && npm run cjs:vue",
    "cjs:index": "babel src/components/entry/index.tsx -o package/cjs/index.js --extensions \".js,.jsx,.ts,.tsx\" --config-file \"./build/cjs.babelrc\"",
    "cjs:vue": "babel src/components/entry/vue.ts -o package/cjs/vue.js --extensions \".js,.jsx,.ts,.tsx\" --config-file \"./build/cjs.babelrc\"",
    "esm": "cross-env NODE_ENV=production FORMAT=es rollup -c build/rollup.config.js -o package/es/wc-app.js --inlineDynamicImports && npm run esm:index && npm run esm:vue",
    "esm:index": "babel src/components/entry/index.tsx -o package/es/index.js --extensions \".js,.jsx,.ts,.tsx\" --config-file \"./build/es.babelrc\"",
    "esm:vue": "babel src/components/entry/vue.ts -o package/es/vue.js --extensions \".js,.jsx,.ts,.tsx\" --config-file \"./build/es.babelrc\"",
    "dts": "rollup -c build/rollup.config.dts.js -o package/types/index.d.ts",
    "style": "rollup -c build/rollup.config.style.js -o style/useless.js"
  },
  "dependencies": {
    "@emotion/cache": "^11.4.0",
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "antd": "^4.16.13",
    "axios": "^0.21.1",
    "insert-css": "^2.0.0",
    "mobx": "^6.0.4",
    "mobx-react-lite": "^3.1.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.1",
    "react-styled-shadow-dom": "^1.0.0",
    "rollup-plugin-polyfill-node": "^0.7.0",
    "styled-components": "^5.3.1",
    "universal-cookie": "^4.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-image": "^2.0.5",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@types/classnames": "^2.2.10",
    "@types/lodash": "^4.14.161",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "babel-plugin-import": "^1.13.1",
    "cross-env": "^7.0.2",
    "eslint": "^7.9.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "^4.1.2",
    "less": "^3.12.2",
    "prettier": "^2.1.2",
    "rollup-plugin-analyzer": "^3.3.0",
    "rollup-plugin-dts": "^1.4.13",
    "rollup-plugin-ignore-import": "^1.3.2",
    "rollup-plugin-license": "^2.2.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-terser": "^7.0.2",
    "stylelint": "^13.7.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "4.2.3",
    "vite": "^2.4.4"
  }
}
